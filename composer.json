{
    "name": "ccns/ccns-ecommerce-cart",
    "description": "A Laravel package for e-commerce cart management.",
    "version": "0.1",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Coconut Soft Creation",
            "email": "email@example.com"
        }
    ],
    "require-dev": {
        "php": "^8.2",
        "illuminate/support": "^v11.0",
        "illuminate/database": "^v11.0",
        "symfony/console": "^7.0",
        "orchestra/testbench": "^9.0",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.4"
    },
    "autoload": {
        "psr-4": {
            "Ccns\\CcnsEcommerceCart\\": "src/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ccns\\CcnsEcommerceCart\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ccns\\CcnsEcommerceCart\\CartServiceProvider"
            ],
            "aliases": {
                "Cart": "Ccns\\CcnsEcommerceCart\\Facades\\Cart"
            }
        },
        "branch-alias": {
            "dev-main": "1.x"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
